{
    "name": "starter-app",
    "version": "1.0.0",
    "description": "StarterApp | SheetRocks Starter App",
    "scripts": {
        "build": "parcel build --no-scope-hoist --no-optimize"
    },
    "repository": {
        "type": "git"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/node": "^18.15.5",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "axios": "^1.3.4",
        "dotenv": "^16.3.1",
        "local-ssl-proxy": "^2.0.5",
        "lodash": "^4.17.21",
        "react": "latest",
        "react-dom": "latest"
    },
    "engines": {
        "node": ">= 16"
    },
    "devDependencies": {
        "@fortawesome/free-solid-svg-icons": "latest",
        "@fortawesome/react-fontawesome": "latest",
        "@parcel/transformer-sass": "latest",
        "@parcel/transformer-typescript-tsc": "latest",
        "buffer": "^5.7.1",
        "parcel": "latest",
        "sass": "latest"
    },
    "targets": {
        "web": {
            "source": [
                "web/index.tsx",
                "web/index.scss",
                "web/login.tsx",
                "web/login.scss"
            ],
            "distDir": "web/dist",
            "context": "browser"
        },
        "webhooks": {
            "source": [
                "api/user-info.ts"
            ],
            "distDir": "api/dist",
            "outputFormat": "esmodule",
            "context": "node",
            "isLibrary": true
        },
        "automations": {
            "source": [
                "automations/automation.ts"
            ],
            "distDir": "automations/dist",
            "outputFormat": "esmodule",
            "context": "node",
            "isLibrary": true
        }
    }
}
